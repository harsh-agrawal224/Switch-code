import java.util.*;
class Switch{
	public static void main(String args[]){
		int choice;
		System.out.println("Enter your choice : 1. hie \t 2. hey \t 3. hello");
		Scanner ob = new Scanner(System.in);
		choice=ob.nextInt();
		switch(choice){
			case 1 :
			System.out.print("You choosed hie");
			break;
			case 2 :
			System.out.print("You choosed hey");
			break;
			case 3:
			System.out.print("You choosed hello");
			break;
			default :
			System.out.print("You choosed wrog option");
		} 
	
	}
}
